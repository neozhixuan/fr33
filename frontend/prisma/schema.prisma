generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model users {
  id                Int       @id @default(autoincrement())
  name              String
  email             String    @unique
  password          String
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  registration_step Int       @default(1) @db.SmallInt
}
